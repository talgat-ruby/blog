{
  "printWidth": 80,
  "singleQuote": true,
  "trailingComma": "none",
  "bracketSpacing": false,
  "jsxBracketSameLine": false,
  "parser": "babylon",
  "semi": true,
  "requirePragma": false,
  "proseWrap": "preserve",
  "arrowParens": "avoid",
  "overrides": [
    {
      "files": [".prettierrc", ".eslintrc", "package.json"],
      "options": {"parser": "json"}
    }
  ]
}
